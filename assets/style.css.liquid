-------------------------------------*/

.carousel .block-title, .carousel .block-text {
  text-shadow: 1px 1px 5px rgba(0, 0, 0, 1);
}

.carousel .block-title {
  font-family: "{{ type_homepage_slideshow_header_fontFamily }}";
  font-size: {{ type_homepage_slideshow_header_fontSize }};
  color: {{ type_homepage_slideshow_header_color }};
  font-weight: {{ type_homepage_slideshow_header_weight }};
  font-style: {{ type_homepage_slideshow_header_style }}; 
}

.carousel .block-text {
  font-family: "{{ type_homepage_slideshow_text_fontFamily }}";
  font-size: {{ type_homepage_slideshow_text_fontSize }};
  color: {{ type_homepage_slideshow_text_color }};
  font-weight: {{ type_homepage_slideshow_text_weight }};
  font-style: {{ type_homepage_slideshow_text_style }}; 
}

.carousel.fade {
  opacity: 1;
}
.carousel.fade .item {
  -moz-transition: opacity ease-in-out .7s;
  -o-transition: opacity ease-in-out .7s;
  -webkit-transition: opacity ease-in-out .7s;
  transition: opacity ease-in-out .7s;
  left: 0 !important;
  opacity: 0;
  top:0;
  position:absolute;
  width: 100%;
  display:block !important;
  z-index:1;
}
.carousel.fade .item:first-child {
  top:auto;
  position:relative;
}
.carousel.fade .item.active {
  opacity: 1;
  -moz-transition: opacity ease-in-out .7s;
  -o-transition: opacity ease-in-out .7s;
  -webkit-transition: opacity ease-in-out .7s;
  transition: opacity ease-in-out .7s;
  z-index:2;
}

/* CAROUSEL BUTTONS LEFT RIGHT */
.carousel-control {
	z-index: 3;
}

.carousel-control .icon-prev,
.carousel-control .icon-next,
.carousel-control .glyphicon-chevron-left,
.carousel-control .glyphicon-chevron-right {
  position: absolute;
  top: 50%;
  z-index: 5;
  display: inline-block;
}
.carousel-control .icon-prev,
.carousel-control .glyphicon-chevron-left {
  left: 20%;
  margin-left: -10px;
  font-size: 1em;
}
.carousel-control .icon-next,
.carousel-control .glyphicon-chevron-right {
  margin-right: -10px;
  font-size: 1em;
  right: 20%;
}
.carousel-control .icon-prev,
.carousel-control .icon-next {
  width: 20px;
  height: 20px;
  margin-top: -10px;
}


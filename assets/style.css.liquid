.aboutus_execprofile_profile_wrapper {
	margin: 15px 7px;
	padding: 0px;
	display: inline-block;
	text-align: center;
	vertical-align: top;
	font-size: 1em;
	line-height: 1.375em;

}

.aboutus_execprofile_profile_info {
	width: 200px;
	height: auto;
	position: relative;
}

.aboutus_execprofile_profile_info img {
	width: 100%;
	height: auto;
	border-radius: 1%;
}

/* inner text that shows up in the picture */
.aboutus_execprofile_profile_description {
	color: rgba(0,0,0,0);
	position: absolute;
	top: 1px;
	left: 1px;
	padding: 5px 10px;
	font-size: 1em;
	line-height: 1.3;
}


.aboutus_execprofile_profile_name {
	font-size: 1.1em;
	margin-top: 10px;
	font-weight: bold;
}


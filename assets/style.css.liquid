/* mobile first */
.eventspage_container {
	width: 100%;
	margin-left: auto;
	margin-right: auto;
	text-align: center;
}

.eventspage_page_title {
  margin-top: 20px;

  font-family: "{{ eventspage_type_page_title_fontFamily }}";
  font-size: {{ eventspage_type_page_title_fontSize }};
  color: {{ eventspage_type_page_title_color }};
  font-weight: {{ eventspage_type_page_title_weight }};
  font-style: {{ eventspage_type_page_title_style }}; 
}

.eventspage_page_description {
  font-family: "{{ eventspage_type_page_text_fontFamily }}";
  font-size: {{ eventspage_type_page_text_fontSize }};
  color: {{ eventspage_type_page_text_color }};
  font-weight: {{ eventspage_type_page_text_weight }};
  font-style: {{ eventspage_type_page_text_style }}; 
}

.eventspage_divider {
	width: 100%;
	border: 2px solid #A32638;
	margin: 15px 0px;
}

/* tablet? 15 + 250 + 15 + 15 + 250 + 15*/
@media screen and (min-width: 590px) {
	.eventspage_container {
		width: 580px;
		margin: 0px auto;
	}
}

/* desktop? 15 + 250 + 15 + 15 + 250 + 15 + 15 + 250 + 15 === 840px */
@media screen and (min-width: 880px) {
	.eventspage_container {
		width: 850px;
		margin: 0px auto;
	}
}

.eventspage_event_wrapper {
	margin: 15px;
	padding: 0px;
	display: inline-block;
	text-align: center;
	vertical-align: top;
	font-size: 1.5em;
}

.eventspage_event_info {
	width: 250px;
	position: relative;
}

.eventspage_event_info img {
	width: 100%;
	height: auto;
	border-radius: 50%;
	border: 4px solid #A32638;
}

/* inner text that shows up in the picture */
.eventspage_event_description {
	color: rgba(0,0,0,0);
	position: absolute;
	top: 1px;
	left: 1px;
	margin: 37.5px;
	text-align: center;
	font-size: 0.75em;
}

.eventspage_event_title {
	position: absolute;
	top: 50%;
	left: 2.5px;
	width: 98%;
	background: rgba(70,70,70,0.8);
	color: rgba(255,255,255,0.9);
	border-radius: 30%;
}

.eventspage_event_title {
  font-family: "{{ eventspage_type_event_name_fontFamily }}";
  font-size: {{ eventspage_type_event_name_fontSize }};
  color: {{ eventspage_type_event_name_color }};
  font-weight: {{ eventspage_type_event_name_weight }};
  font-style: {{ eventspage_type_event_name_style }}; 
}

.eventspage_event_description {
  font-family: "{{ eventspage_type_event_text_fontFamily }}";
  font-size: {{ eventspage_type_event_text_fontSize }};
  color: {{ eventspage_type_event_text_color }};
  font-weight: {{ eventspage_type_event_text_weight }};
  font-style: {{ eventspage_type_event_text_style }}; 
}

.eventspage_event_date {
	font-size: 0.75em;
	margin-bottom: 5px;

  font-family: "{{ eventspage_type_event_date_fontFamily }}";
  font-size: {{ eventspage_type_event_date_fontSize }};
  color: {{ eventspage_type_event_date_color }};
  font-weight: {{ eventspage_type_event_date_weight }};
  font-style: {{ eventspage_type_event_date_style }}; 
}

.eventspage_event_rsvp_link {
	text-decoration: none;

  font-family: "{{ eventspage_type_event_rsvp_fontFamily }}";
  font-size: {{ eventspage_type_event_rsvp_fontSize }};
  color: {{ eventspage_type_event_rsvp_color }};
  font-weight: {{ eventspage_type_event_rsvp_weight }};
  font-style: {{ eventspage_type_event_rsvp_style }}; 
}

.eventspage_event_info:hover img {
	opacity: 0.15;
	-webkit-transition: opacity 0.1s ease-in-out;
	-moz-transition: opacity 0.1s ease-in-out;
	-ms-transition: opacity 0.1s ease-in-out;
	-o-transition: opacity 0.1s ease-in-out;
	transition: opacity 0.1s ease-in-out;
}

.eventspage_event_info:hover .eventspage_event_title {
	opacity: 0.1;
}

.eventspage_event_info:hover p {
	color: rgba(0,0,0,1);
	-webkit-transition: opacity 0.2s ease-in-out;
	-moz-transition: opacity 0.2s ease-in-out;
	-ms-transition: opacity 0.2s ease-in-out;
	-o-transition: opacity 0.2s ease-in-out;
	transition: opacity 0.2s ease-in-out;
}


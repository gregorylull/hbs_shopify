/******************************************************************************
  DEBUGGER PANEL and MOBILE CHECK 
    + hbsDebuggerSwitch are for the styling buttons, can turn on in settings
    + mobile check is for internal javascript code which uses @media to accurately check what the current viewport is classified as
  tags: *debugger* *mobile*    
******************************************************************************/

.hbsDebuggerPanel::after {
	content: "";
	display: block;
	width: 100%;
	line-height: 0;
	font-size: 0;
}

.hbsDebuggerSwitch {
	margin: 0px 1px;
	border: 1px dotted blue;
}

.background_test {
	background-color: yellow;
	text-align: center;
}

.foreground_test {
	background-color: gray;
	text-align: center;
}

.background_test div {
	display: inline-block;
	padding: 0 5px;
	background-color: rgb(200, 200, 200);
	border: 2px solid black;
}

.foreground_test div {
	display: inline-block;
	padding: 0 5px;
	background-color: rgb(220, 220, 200);
	border: 2px solid black;
}

/* The following is for internal javascript to check for viewport classification */

.mobileCheck { background-color: red;}

/* small devices - tablets */
@media screen and (min-width: 768px) {
	.mobileCheck { background-color: green;	}
}

/* medium - desktpo*/
@media screen and (min-width: 992px) {
	.mobileCheck { background-color: blue; }
}

/* large desktops */
@media screen and (min-width: 1200px) {
	.mobileCheck { background-color: white; }
}


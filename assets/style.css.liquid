/******************************************************************************
  Universal
    - {{ type_default_sectionHeader_ }}
    - {{ type_default_page_ }}
******************************************************************************/
body {
  font-family: "{{ type_default_page_fontFamily }}";
  color: {{ type_default_page_color }};
  font-weight: {{ type_default_page_weight }};
  font-style: {{ type_default_page_style }}; 
}

body h1, body h2, body h3, body h4, body h5, body h6 {
  font-family: "{{ type_default_sectionHeader_fontFamily }}";
  color: {{ type_default_sectionHeader_color }};
  font-weight: {{ type_default_sectionHeader_weight }};
  font-style: {{ type_default_sectionHeader_style }};
}

.container, .container-fluid, #page_container {
	margin-bottom: 40px;
}

img {
	width: 100%;
	height: auto;
}

.gl_clearfix::after {
	display: block;
	content: " ";
	clear: both;
	line-height: 0;
	font-size: 0;
}

@media screen and (min-width: 768px) {
}

/******************************************************************************
  HEADER / BANNER / LOGO
******************************************************************************/

.hbs_headers {
	padding-top: 10px;
	box-shadow: 0px 5px 2px -2px rgba(200,200,200,0.75);
}

/* FOR FRONT PAGE ONLY, header will have NO bottom shadow, conflicts with slideshow pic*/
.hbs_headers.index {
	box-shadow: none;
}

.hbs_headers.index .navbar {
	margin-bottom: 0px;
}

/*-------------------------------------
  Navigation bar
--------------------------------------*/
.navbar-toggle span.icon-bar {
	background-color: black;
}

.hbs_headers .navbar-brand {
	padding: 5px;
	width: 75%;
}

.hbs_headers .navbar_logo {
  height: 75%;
  width: auto;
}

.hbs_headers .navbar_logoWords {
	width: 80%;
	height: auto;
}

@media (max-width: 767px) {
  .navbar-nav.navbar-right {
    float: right;
  }
}

@media screen and (min-width: 768px) {
  .hbs_headers.index .navbar {
	  margin-bottom: 20px;
  }

	.hbs_headers .navbar-brand {
		width: auto;
	}
	.hbs_headers .navbar_logo {
		width: auto;
		height: 100%;
	}
	.hbs_headers .navbar_logoWords {
		width: auto;
		height: 40px;
	}

	.hbs_headers {
	  padding-top: 25px;
	  padding-bottom: 15px;
  }
}

@media (max-width: 1100px) {
    .navbar-header {
      float: none;
    }

    .navbar-toggle {
      display: block;
    }
    .navbar-collapse {
      border-top: 1px solid transparent;
      box-shadow: inset 0 1px 0 rgba(255,255,255,0.1);
    }
    .navbar-fixed-top {
			top: 0;
			border-width: 0 0 1px;
		}
    .navbar-collapse.collapse {
        display: none!important;
    }
    .navbar-nav {
			margin-top: 7.5px;
		}
	  .navbar-nav > li {
      float: left;
    }
    .navbar-nav > li > a {
      padding-top: 10px;
      padding-bottom: 10px;
    }
    .collapse.in{
  		display:block !important;
		}
}


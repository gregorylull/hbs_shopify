
/******************************************************************************
  Universal
******************************************************************************/

body, .container, .container-fluid, #page_container {
	margin-bottom: 40px;
	/*font-family: "Lato", sans-serif;*/
}

img {
	width: 100%;
	height: auto;
}

.gl_clearfix::after {
	display: block;
	content: " ";
	clear: both;
	line-height: 0;
	font-size: 0;
}

@media screen and (min-width: 768px) {
}

/******************************************************************************
  HEADER / BANNER / LOGO
******************************************************************************/

.hbs_headers {
	padding-top: 10px;
	/*padding-bottom: 10px;*/
	box-shadow: 0px 5px 2px -2px rgba(200,200,200,0.75);
}

/* FOR FRONT PAGE ONLY, header will have NO bottom shadow, conflicts with slideshow pic*/
.hbs_headers.index {
	box-shadow: none;
}

.hbs_headers.index .navbar {
	margin-bottom: 0px;
}

/*-------------------------------------
  Navigation bar
--------------------------------------*/
.navbar-toggle span.icon-bar {
	background-color: black;
}

.hbs_headers .navbar-brand {
	padding: 5px;
	width: 75%;
}

.hbs_headers .navbar_logo {
  height: 75%;
  width: auto;
}

.hbs_headers .navbar_logoWords {
	width: 80%;
	height: auto;
}

@media screen and (min-width: 768px) {
  .hbs_headers.index .navbar {
	  margin-bottom: 20px;
  }

	.hbs_headers .navbar-brand {
		width: auto;
	}
	.hbs_headers .navbar_logo {
		width: auto;
		height: 100%;
	}
	.hbs_headers .navbar_logoWords {
		width: auto;
		height: 40px;
	}

	.hbs_headers {
	  padding-top: 25px;
	  padding-bottom: 15px;
  }
}


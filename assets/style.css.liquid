/******************************************************************************
  ABOUT US PAGE: EXEC TEAM PROFILE
******************************************************************************/
#aboutus_video {
	position: fixed;
	top: 0px;
	left: 0px;
	min-width: 100%;
	min-height: 100%;
	width: auto;
	height: auto;
	z-index: -1000;
	overflow: hidden;
	opacity: 0.5;
}

/* mobile first */
.aboutus_execprofile_container {
	width: 100%;
	margin: 0px auto;
	text-align: center;
	font-family: 'Pontano Sans', sans-serif;
}

.aboutus_execprofile_page_title {
  margin-top: 0px;
}

.aboutus_execprofile_page_description {
	text-align: justify;
	padding: 20px 10%;
}

div.aboutus_execprofile_divider {
	width: 47%;
	border: 1px dotted rgb(220,220,220);
	margin: 15px auto;
}

.aboutus_execprofile_meet_blurb {
	text-align: center;
}

/* tablet? 7 + 200 + 7 + 7 + 200 + 7 === 428*/
@media screen and (min-width: 590px) {
	.aboutus_execprofile_container {
		width: 580px;
		margin: 0px auto;
	}
}

/* desktop? 15 + 200 + 15 + 15 + 200 + 15 + 15 + 200 + 15 + 15 + 200 + 15=== 920px */
@media screen and (min-width: 880px) {
	.aboutus_execprofile_container {
		width: 875px;
		margin: 0px auto;
	}
}

.aboutus_execprofile_profile_wrapper {
	margin: 15px 7px;
	padding: 0px;
	display: inline-block;
	text-align: center;
	vertical-align: top;
	font-size: 1em;
	line-height: 1.375em;

}

.aboutus_execprofile_profile_info {
	width: 200px;
	height: auto;
	position: relative;
}

.aboutus_execprofile_profile_info img {
	width: 100%;
	height: auto;
	border-radius: 1%;
}

/* inner text that shows up in the picture */
.aboutus_execprofile_profile_description {
	color: rgba(0,0,0,0);
	position: absolute;
	top: 1px;
	left: 1px;
	padding: 5px 10px;
	font-size: 1em;
	line-height: 1.3;
}


.aboutus_execprofile_profile_name {
	font-size: 1.2em;
	margin-top: 10px;
}

#execprofile_position-break.aboutus_execprofile_position-break {
	width: 60%;
	height: 1px;
	background-color: rgb(97, 145, 16);
	margin: 3px auto;
}

.aboutus_execprofile_profile_position {
	font-size: 1em;
}


.aboutus_execprofile_profile_email {
	text-decoration: none;
	font-size: 1em;
}

.aboutus_execprofile_profile_socialIcons span{
	font-size: 1.5em;
}

.aboutus_execprofile_profile_info:hover img {
	opacity: 0.15;
	-webkit-transition: opacity 0.1s ease-in-out;
	-moz-transition: opacity 0.1s ease-in-out;
	-ms-transition: opacity 0.1s ease-in-out;
	-o-transition: opacity 0.1s ease-in-out;
	transition: opacity 0.1s ease-in-out;
}

.aboutus_execprofile_profile_info:hover .aboutus_execprofile_profile_name,
.aboutus_execprofile_profile_info:hover .aboutus_execprofile_profile_position,
.aboutus_execprofile_profile_info:hover .aboutus_execprofile_position-break,
.aboutus_execprofile_profile_info:hover .aboutus_execprofile_profile_email {
	opacity: 0.1;
	-webkit-transition: opacity 0.1s ease-in-out;
	-moz-transition: opacity 0.1s ease-in-out;
	-ms-transition: opacity 0.1s ease-in-out;
	-o-transition: opacity 0.1s ease-in-out;
	transition: opacity 0.1s ease-in-out
}

.aboutus_execprofile_profile_info:hover p {
	color: rgba(0,0,0,1);
  -webkit-transition: opacity 0.1s ease-in-out;
  -moz-transition: opacity 0.1s ease-in-out;
  -ms-transition: opacity 0.1s ease-in-out;
  -o-transition: opacity 0.1s ease-in-out;
  transition: opacity 0.1s ease-in-out;
}

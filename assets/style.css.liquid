/*-------------------------------------
  UPCOMING EVENTS GRID
--------------------------------------*/
.frontpage_md_upcomingEvents .eventPicture {
	width: 100%;
	height: auto;
}

/*-------------------------------------
  FEATURED PRODUCT GRID FRONT PAGE
--------------------------------------*/

.featuredProductContainer {
	text-align: center;
}

.featuredProductContainer .productWrapper {
	display: inline-block;
	margin: 15px auto;
	border: 1px dotted rgb(240, 240, 240);
	width: 28%;
}

.featuredProductContainer::after {
	content: "";
	display: inline-block;
	width: 100%;
}

.featuredProductContainer .productPicture {
	width: auto;
	height: auto;
}

.featuredProductContainer .productTitle {
	text-align: center;
}

/*-------------------------------------
  MEDIA QUERIES
-------------------------------------*/

@media screen and (min-width: 768px) {
  .container.index {
  	max-width: 1000px;
  }

	.frontpage_md_leftColumn h3 {
		text-align: left;
	}

	/*----------------------
	  UPCOMING EVENTS
	-----------------------*/
	.frontpage_md_upcomingEvents .eventPicture {
		max-width: 200px;
		min-width: 150px;
		height: auto;
  }
  .frontpage_md_upcomingEvents .img-left {
  	background-image: linear-gradient(to right, rgb(164, 16, 52), rgb(220,220,220) 10%, white);
  }
  .frontpage_md_upcomingEvents .img-right {
  	background-image: linear-gradient(to left, rgb(164, 16, 52), rgb(220,220,220) 10%, white);
  }
}

